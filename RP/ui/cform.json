  "hh1": {
    "type": "image",
    "texture": "textures/ore-form/background/form",
    "size":[284,
      200],
    "controls":[{
      "stacked": {
        "type":"stack_panel",
        "size":["100%",
          "100%"],
        "controls":[{
          "header": {
            "type": "panel",
            "size":["100%",
              20],
            "controls":[{
              "background": {
                "type": "image",
                "size": ["100%",
                  "100%"],
                "texture": "textures/ui/Black",
                "controls":[{
                  "black": {
                    "type":"image",
                    "texture": "textures/ore-form/background/header",
                    "size":["100%-2px",
                      "100%-2px"]
                  }
                },
                  {
                    "title_label": {
                      "type": "label",
                      "layer": 30,
                      "offset":[0,
                        -3],
                      "text": "#title_text",
                      "font_type": "MinecraftTen"
                    }
                  },
                  {
                    "close_button@common.close_button": {
                      "offset":[0,
                        -2],
                      "$close_button_default_texture": "textures/ui/close_button_default_light",
                      "$hover_texture": "textures/ui/close_X_button_hover",
                      "$pressed_texture": "textures/ui/close_X_button_hover",
                      "layer": 3
                    }
                  }]
              }
            }]
          }
        },
          {
            "form": {
              "type": "panel",
              "size":["100%",
                "fill"],
              "controls":[{
                "overlay_form": {
                  "type": "image",
                  "size":["100%-10px",
                    "100%-10px"],
                  "texture": "textures/ore-form/background/overlay_form",
                  "controls":[{
                    "root_panel": {
                      "type": "panel",
                      "size":["100%-4px",
                        "100%-4px"],
                      "clips_children": true,
                      "controls":[{
                        "overlay": {
                          "type": "image",
                          "texture": "textures/overlay",
                          "size":["200%",
                            "200%"]
                        }
                      },
                        {
                          "root": {
                            "type": "panel",
                            "size":["100%-4px",
                              "100%"],
                            "controls":[{
                              "scrolling_panel@common.scrolling_panel": {
                                "anchor_to": "top_left",
                                "anchor_from": "top_left",
                                "$show_background": false,
                                "size": ["100%",
                                  "100%"],
                                "$scrolling_content": "nxyz_ui.rank_scrolling_content",
                                "$scroll_size": [5,
                                  "100% - 4px"],
                                "$enabled_bar": false,
                                "$scrolling_pane_size": ["100% - 4px",
                                  "100% - 2px"],
                                "$scrolling_pane_offset": [2,
                                  0],
                                "$scroll_bar_right_padding_size": [0,
                                  0]
                              }
                            }]
                          }
                        }]
                    }
                  }]
                }
              }]
            }
          }]
      }
    }],
    "bindings": [{
      "binding_name": "#title_text"
    },
      {
        "binding_type": "view",
        "source_property_name": "(not ((#title_text - '§h§h§1') = #title_text))",
        "target_property_name": "#visible"
      }]
